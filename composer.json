{
    "name": "huubl/silverstripe-elemental-timeline",
    "type": "silverstripe-vendormodule",
    "description": "Display timeline",
    "keywords": [
        "silverstripe", "elemental", "blocks", "timeline"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Huubl",
            "email": "contact@studiolunter.nl"
        }
    ],
    "require": {
        "dnadesign/silverstripe-elemental": "^2@dev || ^3@dev",
        "dynamic/silverstripe-elemental-baseobject": "^1@dev",
        "silverstripe/vendor-plugin": "^1@dev",
        "sheadawson/silverstripe-linkable": "^2@dev",
        "symbiote/silverstripe-gridfieldextensions": "^3.1@dev"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "expose": [
            "icons"
        ],
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Huubl\\Elements\\Timeline\\": "src/",
            "Huubl\\Elements\\Timeline\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint": "vendor/bin/phpcs src/ tests/",
        "lint-clean": "vendor/bin/phpcbf src/ tests/"
    }
}
